{
  "lock": false,
  "imports": {
    "#/": "./src/",
    "@ai-sdk/google": "npm:@ai-sdk/google@^2.0.43",
    "@comunica/query-sparql": "npm:@comunica/query-sparql@^4.5.0",
    "@comunica/types": "npm:@comunica/types@^4.5.0",
    "@rdfjs/types": "npm:@rdfjs/types@^2.0.1",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "ai": "npm:ai@^5.0.102",
    "oxigraph": "npm:oxigraph@^0.5.2",
    "ts-to-zod": "npm:ts-to-zod@^5.1.0",
    "zod": "npm:zod@^4.1.13",
    "sparqljs": "npm:sparqljs@^3.7.3"
  },
  "tasks": {
    "start": "deno -A --env main.ts",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno check",
    "test": "deno test --allow-read",
    "precommit": {
      "description": "Run all pre-commit tasks",
      "dependencies": [
        "fmt",
        "lint",
        "check",
        "test"
      ]
    }
  }
}
