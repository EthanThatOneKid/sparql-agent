{
  "lock": false,
  "imports": {
    "#/": "./src/"
  },
  "tasks": {
    "start": "deno -A --env main.ts",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno check",
    "precommit": {
      "description": "Run all pre-commit tasks",
      "dependencies": [
        "fmt",
        "lint",
        "check"
      ]
    }
  }
}
